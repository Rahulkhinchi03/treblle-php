{
    "type": "library",
    "name": "treblle/treblle-php",
    "description": "Stay in tune with your APIs",
    "homepage": "https://treblle.com/",
    "keywords": [
        "phpunit",
        "treblle",
        "api",
        "monitoring",
        "debuging",
        "documentation"
    ],
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Vedran CindriÄ‡",
            "email": "vedran@treblle.com",
            "homepage": "https://treblle.com/"
        },
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://treblle.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Treblle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Treblle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "php-http/client-common": "^2.6",
        "php-http/discovery": "^1.15.2",
        "php-http/mock-client": "^1.5",
        "treblle/treblle-sdk-core": "0.0.2"
    },
    "require-dev": {
        "justinrainbow/json-schema": "^5.2.12",
        "nyholm/psr7": "^1.5.1",
        "pestphp/pest": "^2.2.1",
        "phpstan/phpstan": "^1.10",
        "symfony/http-client": "^6.2.7"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    }
}
